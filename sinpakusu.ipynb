    "#aに実際の心拍数を入れます。bに見積もった心拍数を入れます\n",
    "a = 56\n",
    "b = 64\n",
    "\n",
    "c = 1 - (abs(a - b) / ((a + b) / 2))\n",
    "print(\"あなたの内受容間隔は\", c,\"です\\n\")\n",
    "print(\"0.6以下で平均未満、0.7以上で平均以上です\")"
