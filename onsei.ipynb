    "import speech_recognition as sr\n",
    " \n",
    "r = sr.Recognizer()\n",
    " \n",
    "with sr.AudioFile(\"sample1.wav\") as source:\n",
    "    audio = r.record(source)\n",
    " \n",
    "text = r.recognize_google(audio, language='ja-JP')\n",
    " \n",
    "print(text)"
